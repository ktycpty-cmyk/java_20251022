package test;

public class Example01 {

	public static void main(String[] args) {

		
		/*
		 * ---------------출력 예시---------------
		 * 이름: 본인이름         
		 * 나이: 25             : age 
		 * 주소: 서울시 천호동     : address 
		 * 몸무게: 77.5kg        : weight
		 
		 * 
		 * 
		 * 
		 */
		
	  String name = "김상준";
	  System.out.println("김상준: " + name);
	  
	  int age = 27;
	  System.out.println("27: " + age);
	  
	  String address = "강동구 천호동";
	  System.out.println("강동구 천호동: " + address);
	  
	  double weight = 75.4;
	  System.out.println("75.4: " + weight +"kg");
	  
	  
		
		
	}

}
